INTERLIS 2.3;
    
MODEL My_Model (en)
AT "mailto:gcarrillo@linuxmail.org"
VERSION "2019-12-11" =
  IMPORTS ISO19107_PLANAS_V1;

  TOPIC My_Topic =
      
    CLASS clima =
      id : MANDATORY 1 .. 2147483647;
      nombre : MANDATORY TEXT*100;
      geometria : MANDATORY GM_Point3D;
    END clima;

    CLASS fase_cartografica =
      id : MANDATORY 1 .. 2147483647;
      simbolo : MANDATORY TEXT*8;
      id_pendiente : MANDATORY 1 .. 2147483647;
      id_pedregosidad : 1 .. 2147483647;
      id_salinidad : 1 .. 2147483647;
      id_erosion : 1 .. 2147483647;
      id_inundabilidad : 1 .. 2147483647;
      id_unidad_cartografica : MANDATORY 1 .. 2147483647;
      frag_sup : MANDATORY 1 .. 2147483647;
      frag_suelo : MANDATORY 1 .. 2147483647;
      capacidad : MANDATORY TEXT*50;
      vocacion : MANDATORY TEXT*50;
      geometrie : GM_MultiSurface3D;
    END fase_cartografica;

  END My_Topic;
      
END My_Model.
